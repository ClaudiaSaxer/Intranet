@model Intranet.Labor.ViewModel.LaborCreatorViewModel

@{
    ViewBag.Title = "Intigena Intranet - Labor Module";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="jumbotron">
    <div class="container">
        <h1>Testresultate Höschenwindeln - @Model.FaNr </h1>
        <div class="row">


            <div class="col-xs-12 col-sm-6 col-lg-3">
                <p>Produkt: @Model.ProductName</p>

            </div>
            <div class="col-xs-12 col-sm-6 col-lg-3">
                <p>Grösse: @Model.SizeName</p>

            </div>
            <div class="col-xs-12 col-sm-6 col-lg-3">
                <p>Datum: @Model.CreatedDate</p>

            </div>
            <div class="col-xs-12 col-sm-6 col-lg-3">
                <p>Schicht: @Model.Shift</p>

            </div>
        </div>
    </div>


</div>


<div class="body-content">

    <button type="button" class="btn btn-primary btn-lg">
        <span class="glyphicon glyphicon glyphicon-plus" aria-hidden="true"></span> Rewet
    </button>

    <button type="button" class="btn btn-primary btn-lg">
        <span class="glyphicon glyphicon glyphicon-plus" aria-hidden="true"></span> Retention
    </button>

</div>

<div class="body-content">
    <h2>Revet</h2>
    <table id="example" class="table table-striped table-bordered" cellspacing="0" width="100%">
        <thead>
        <tr>
            <th>Prod Code</th>
            <th>Gewicht trocken</th>
            <th>nach 140 ml</th>
            <th>RW</th>
            <th>nach 210 ml</th>
            <th>RW</th>
            <th>Stricke Trough</th>
            <th>Verteilung</th>
            <th>Testperson</th>
            <td>Editieren</td>
            <td>Löschen</td>

        </tr>
        </thead>
        <tbody>
        @foreach ( var item in Model.Rewets )
        {
            <tr>
                <td>@item.TestInfo.ProductionCode</td>
                <td>@item.TestInfo.WeightyDiaperDry</td>
                <td>@item.Rewet.Revet140Value</td>
                <td>@item.Rewet.Revet140Rw</td>
                <td>@item.Rewet.Revet210Value</td>
                <td>@item.Rewet.Revet210Rw</td>
                <td>@item.Rewet.StrikeTroughValue</td>
                <td>@item.Rewet.DistributionOfTheStrikeTrough</td>
                <td>@item.TestInfo.TestPerson</td>
                <td>
                    <a href="#" class="btn btn-default btn-lg">
                        <span class="glyphicon glyphicon-pencil"></span>
                    </a>
                </td>
                <td>
                    <a href="#" class="btn btn-danger btn-lg">
                        <span class="glyphicon glyphicon-trash"></span> 
                    </a>
                </td>
            </tr>
        }
        </tbody>
        <tfoot>
        <tr>
            <th>Durchschnitt</th>
            <th></th>
            <th>@Model.RewetAverage.Revet140Value</th>
            <th>@Model.RewetAverage.Revet140Rw</th>
            <th>@Model.RewetAverage.Revet210Value</th>
            <th>@Model.RewetAverage.Revet210Rw</th>
            <th>@Model.RewetAverage.StrikeTroughValue</th>
            <th>@Model.RewetAverage.DistributionOfTheStrikeTrough</th>
            <th></th>
            <th></th>
            <th></th>
        </tr>
        </tfoot>
        <tfoot>
        <tr>
            <th>Standard Abweichung</th>
            <th></th>
            <th>@Model.RewetStandardDeviation.Revet140Value</th>
            <th>@Model.RewetStandardDeviation.Revet140Rw</th>
            <th>@Model.RewetStandardDeviation.Revet210Value</th>
            <th>@Model.RewetStandardDeviation.Revet210Rw</th>
            <th>@Model.RewetStandardDeviation.StrikeTroughValue </th>
            <th>@Model.RewetStandardDeviation.DistributionOfTheStrikeTrough</th>
            <th></th>
            <th></th>
            <th></th>
        </tr>
        </tfoot>
    </table>
</div>

<div class="body-content">
    <h2>Penetrationszeit</h2>
    <table id="example" class="table table-striped table-bordered" cellspacing="0" width="100%">
        <thead>
        <tr>
            <th>Prod Code</th>
            <th>Gewicht trocken</th>
            <th>Zugabe 1</th>
            <th>Zugabe 2</th>
            <th>Zugabe 3</th>
            <th>Zugabe 4</th>
            <th>Testperson</th>
            <td>Editieren</td>
            <td>Löschen</td>
        </tr>
        </thead> <tbody>
        @foreach ( var item in Model.PenetrationTimes )
        {
            <tr>
                <td>@item.TestInfo.ProductionCode</td>
                <td>@item.TestInfo.WeightyDiaperDry</td>
                <td>@item.PenetrationTime.PenetrationTimeAdditionFirst</td>
                <td>@item.PenetrationTime.PenetrationTimeAdditionSecond</td>
                <td>@item.PenetrationTime.PenetrationTimeAdditionThird</td>
                <td>@item.PenetrationTime.PenetrationTimeAdditionFourth</td>
                <td>@item.TestInfo.TestPerson</td>
                <td>
                    <a href="#" class="btn btn-default btn-lg">
                        <span class="glyphicon glyphicon-pencil"></span>
                    </a>
                </td>
                <td>
                    <a href="#" class="btn btn-danger btn-lg">
                        <span class="glyphicon glyphicon-trash"></span>
                    </a>
                </td>
            </tr>
        }
        </tbody>
        <tfoot>
        <tr>
            <th>Durchschnitt</th>
            <th></th>
            <th>@Model.PenetrationTimeAverage.PenetrationTimeAdditionFirst</th>
            <th>@Model.PenetrationTimeAverage.PenetrationTimeAdditionSecond</th>
            <th>@Model.PenetrationTimeAverage.PenetrationTimeAdditionThird</th>
            <th>@Model.PenetrationTimeAverage.PenetrationTimeAdditionFourth</th>
            <th></th>
            <th></th>
            <th></th>
        </tr>
        </tfoot>
        <tfoot>
        <tr>
            <th>Standard Abweichung</th>
            <th></th>
            <th>@Model.PenetrationTimeStandardDeviation.PenetrationTimeAdditionFirst</th>
            <th>@Model.PenetrationTimeStandardDeviation.PenetrationTimeAdditionSecond</th>
            <th>@Model.PenetrationTimeStandardDeviation.PenetrationTimeAdditionThird</th>
            <th>@Model.PenetrationTimeStandardDeviation.PenetrationTimeAdditionFourth</th>
            <th></th>
            <th></th>
            <th></th>
        </tr>
        </tfoot>
    </table>
</div>

