    <h2>Retention</h2>
    <table id="example" class="table table-striped table-bordered" cellspacing="0" width="100%">
        <thead>
        <tr>
            <th>Prod Code</th>
            <th>Gewicht trocken</th>
            <th>Gewicht nass</th>
            <th>Nach Zentrifuge (g)</th>
            <th>Nach Zentrifuge (%)</th>
            <th>RW</th>
            <th>SAP Typ</th>
            <th>SAP Nr.</th>
            <th>g/Höwi</th>
            <th>Testperson</th>
            <td>Editieren</td>
            <td>Löschen</td>
        </tr>
        </thead>
        <tbody>
        @foreach ( var item in Model.Retentions )
