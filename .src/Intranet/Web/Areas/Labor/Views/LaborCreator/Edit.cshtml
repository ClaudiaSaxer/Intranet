@model Intranet.Labor.ViewModel.LaborCreatorViewModel

@{
    ViewBag.Title = "Intigena Intranet - Labor Module";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="jumbotron">
    <div class="container">
        <h1>Testresultate Höschenwindeln - @Model.FaNr </h1>
        <div class="row">

        
            <div class="col-xs-12 col-sm-6 col-lg-3">
                <p>Produkt: @Model.ProductName</p>

            </div>
            <div class="col-xs-12 col-sm-6 col-lg-3">
                <p>Grösse: @Model.SizeName</p>

            </div>
            <div class="col-xs-12 col-sm-6 col-lg-3">
                <p>Datum: @Model.CreatedDate</p>

            </div>
            <div class="col-xs-12 col-sm-6 col-lg-3">
                <p>Schicht: @Model.Shift</p>

            </div>
        </div>
    </div>
    <button type="button" class="btn btn-default btn-circle btn-lg"><i class="glyphicon glyphicon-ok"></i></button>


</div>