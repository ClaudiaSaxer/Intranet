            <th></th>
            <th>@Model.RetentionAverage.RetentionWetWeight</th>
            <th>@Model.RetentionAverage.RetentionAfterZentrifugeValue</th>
            <th>@Model.RetentionAverage.RetentionAfterZentrifugePercent</th>
            <th>@Model.RetentionAverage.RetentionRw</th>
            <th></th>
            <th></th>
            <th>@Model.RetentionAverage.SapGHoewiValue</th>
            <th></th>
            <th></th>
            <th></th>
        </tr>
        </tfoot>
        <tfoot>
        <tr>
            <th>Standard Abweichung</th>
            <th></th>
            <th>@Model.RetentionStandardDeviation.RetentionWetWeight</th>
            <th>@Model.RetentionStandardDeviation.RetentionAfterZentrifugeValue</th>
            <th>@Model.RetentionStandardDeviation.RetentionAfterZentrifugePercent</th>
            <th>@Model.RetentionStandardDeviation.RetentionRw</th>
            <th></th>
            <th></th>
            <th>@Model.RetentionStandardDeviation.SapGHoewiValue</th>
            <th></th>
            <th></th>
            <th></th>
        </tr>
        </tfoot>
    </table>
</div>