            <tr>
                <td>@item.TestInfo.ProductionCode</td>
                <td>@item.TestInfo.WeightyDiaperDry</td>
                <td>@item.Retention.RetentionWetWeight</td>
                <td>@item.Retention.RetentionAfterZentrifugeValue</td>
                <td>@item.Retention.RetentionAfterZentrifugePercent</td>
                <td>@item.Retention.RetentionRw</td>
                <td>@item.Retention.SapType</td>
                <td>@item.Retention.SapNr</td>
                <td>@item.Retention.SapGHoewiValue</td>
                <td>@item.TestInfo.TestPerson</td>
                <td>
                    <a href="#" class="btn btn-default btn-lg">
